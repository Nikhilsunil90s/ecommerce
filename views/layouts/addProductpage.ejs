<%- include('../includes/header') %>
<!-- Content Body -->
<div class="container">
    <form action="/admin/addProduct" method="POST">
        <div class="form-group">
            <label for="prodName">Name Of Product : </label>
            <input type="text" class="form-control" name="prodName" placeholder="Enter Product Name" id="prodName">
          </div>
          <div class="form-group">
            <label for="price">Price Of Product :</label>
            <input type="text" class="form-control" placeholder="Enter Price" id="price" name="prodPrice">
          </div>
          <button type="submit" class="btn btn-primary">Add Product!</button>
    </form>
</div>
<%- include('../includes/footer') %>
